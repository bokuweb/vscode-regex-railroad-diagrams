{
    "name": "vscode-regex-railroad-diagrams",
    "displayName": "vscode-regex-railroad-diagrams",
    "description": "Display railroad diagram of regex under cursor.",
    "version": "0.0.1",
    "publisher": "kogai",
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "tsc": "$(npm bin)/tsc",
        "eslint": "$(npm bin)/eslint **/*.ts",
        "vscode:prepublish": "$(npm bin)/tsc -p ./",
        "compile": "$(npm bin)/tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test:unit": "node ./node_modules/vscode/bin/test",
        "test": "npm run eslint && npm run tsc && npm run test:unit"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "eslint": "^3.12.2",
        "mocha": "^2.3.3",
        "typescript": "^2.1.4",
        "typescript-eslint-parser": "^1.0.0",
        "vscode": "^1.0.0"
    }
}
